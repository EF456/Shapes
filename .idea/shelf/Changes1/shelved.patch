+            mPath.quadTo(mX, mY, (x + mX) / 2, (y + mY) / 2);
+            mX = x;
+            mY = y;
+        }
+    }   // end touchMoveSpiral
+
+    public void touchUpSpiral() {
+        mPath.lineTo(mX, mY);
+    }
